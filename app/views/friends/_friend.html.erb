<% if friend.user == current_user %>
<tr id="<%= dom_id(friend) %>">
  <td><%= friend.first_name %> <%= friend.last_name %></td>
  <td><%= friend.email %></td>
  <td><%= friend.phone %></td>
  <td><%= friend.twitter %></td>
  <td><%= link_to "Show", friend, class: "btn btn-outline-primary btn-sm" %></td>
</tr>
<% end %>